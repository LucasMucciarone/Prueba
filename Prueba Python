import datetime

# Ajustar la configuraci칩n regional a espa침ol (si est치 disponible en tu sistema)
import locale
try:
    locale.setlocale(locale.LC_TIME, 'es_ES.UTF-3')
except locale.Error:
    pass  # Se mantiene la configuraci칩n regional por defecto si la anterior no existe

hoy = datetime.datetime.now()
print(hoy.strftime('%A, %d de %B de %Y'))
